local gameId = game.PlaceId

local fischIds = {
    16732694052,
    131716211654599  
}
local isFisch = false
for _, id in ipairs(fischIds) do
    if gameId == id then
        isFisch = true
        break
    end
end

if isFisch then
    print("Fisch detected (PlaceId: " .. gameId .. ")! Loading fisch script...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/slowhub235/vhubV1/refs/heads/main/fisch"))()
else
    print("Not Fisch! Loading bladeball script...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/slowhub235/vhubV1/refs/heads/main/bb"))()
end

print("Loader complete!")
